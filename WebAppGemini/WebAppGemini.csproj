    <PackageReference Include="Microsoft.Extensions.Http" Version="10.0.2" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\WebApiGemini.Shared\WebApiGemini.Shared.csproj" />
  </ItemGroup>

</Project>
